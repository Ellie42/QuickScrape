{
  "manifest_version": 2,
  "name": "Quick Scraper",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "css" : [
        "css/style.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}

